{
  "name": "dnd-editor",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "GPL",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "build:plugin": "pnpm -C packages/plugins build",
    "build:core": "pnpm -C packages/core build",
    "build:editor": "pnpm -C packages/editor build",
    "bootstrap": "pnpm build:core && pnpm build:plugin",
    "dev": "pnpm -C packages/editor dev",
    "docs:dev": "pnpm -C packages/docs dev",
    "release:core": "cd packages/core && bumpp --tag core@",
    "release:editor": "cd packages/editor && bumpp --tag editor@",
    "release:plugin": "cd packages/plugins && bumpp --tag plugin@"
  },
  "dependencies": {
    "debug": "^4.4.1",
    "element-plus": "^2.9.11",
    "vue": "^3.5.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@types/debug": "^4.1.12",
    "bumpp": "^10.2.3",
    "eslint": "^9.27.0",
    "typescript": "^5.8.3"
  }
}
